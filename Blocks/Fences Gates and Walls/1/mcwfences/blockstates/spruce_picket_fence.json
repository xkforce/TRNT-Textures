{
  "multipart": [
    {
      "apply": {
        "model": "mcwfences:horse/spruce_log_fence_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "mcwfences:picket/spruce_picket_fence_side",
        "uvlock": false
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "mcwfences:picket/spruce_picket_fence_side",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "mcwfences:picket/spruce_picket_fence_side",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "mcwfences:picket/spruce_picket_fence_side",
        "y": 270,
        "uvlock": false
      }
    }
  ]
}